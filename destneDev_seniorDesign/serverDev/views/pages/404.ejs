<!--
  Page (404) Not Found
-->
<!DOCTYPE html>
<html lang ="en">
  <head>
    <title>Page Not Found</title>
      <link rel="icon" href="/static/images/favicon.ico" type="image/x-icon"/>
    <link rel="stylesheet" href="/static/vendors/stylesheets/bootstrap.min.css">
      <link rel="stylesheet" type="text/css" href="/static/styles/404.css">
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="error-template">
            <h1>
              Oops!
            </h1>
            <h2>
              404 Not Found
            </h2>
              <div class="error-details">
                Sorry, an error has occured, Requested page not found!
              </div>
              <div class="error-actions">
                <a href="javascript:history.go(-1)" class="btn btn-default btn-lg">
                  <span class="glyphicon glyphicon-chevron-left"></span> Go Back </a>
                <a href="/" class="btn btn-primary btn-lg">
                  <span class="glyphicon glyphicon-home"></span>
                  Take Me Home </a>
              </div>
            </div>
          </div>
        </div>
      </div>
  </body>
</html>
